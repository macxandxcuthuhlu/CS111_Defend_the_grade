dic = {2.5: 9, 7.5: 24, 12.5: 54, 17.5: 42, 22.5: 54, 27.5: 68, 32.5: 45, 37.5: 46, 42.5: 27, 47.5: 13}
list = []
for i in dic:
    for m in range(dic[i]):
        list.append(i)
l = len(list)
print(l)
if l % 2 == 0:
    median = (list[int(l/2)] + list[int(l/2)-1]) / 2
else:
    median = list[int(l/2)]
print(median)